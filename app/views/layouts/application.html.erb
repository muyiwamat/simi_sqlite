<!DOCTYPE html>
<html>
<head>
  <title>Aberdeen Locator | <%= yield(:title) %></title>
  <!-- <title><%#= full_title(yield(:title)) %></title> -->
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="wrapper">
	<%# if sign_in? %>
	<% flash.each do |k,v| %>
		<div class="alert alert-<%= k %>"><%= v %></div>
	<% end %>
	<div id="nav">
	</div>
	<%# end %>
	<%= yield %>
</div>

</body>
</html>
